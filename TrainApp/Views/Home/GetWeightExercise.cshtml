
@{
    ViewBag.Title = "GetWeightExercise";
}

<h2>Weight Exercise</h2>

<table class="table table-bordered table-responsive" table-hover>

    <tr>
        <th>Name</th>
        <th>Weight</th>
        <th>Rep</th>
        <th>Set</th>
        <th>Action</th>
    </tr>


    @foreach (var we in ViewBag.weightExerciseList)
    {
        <tr>
            <td>@we.ExerciseName</td>
            <td>@we.Weight</td>
            <td>@we.Rep</td>
            <td>@we.Set</td>
            <td>
                <button type="button" class="btn btn-primary btn-sm">Edit</button> |
                <button type="button" class="btn btn-danger btn-sm">Delete</button>

            </td>
        </tr>
    }


</table>